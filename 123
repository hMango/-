#include <iostream>
using namespace std;

struct Node
{
	int data;
	Node *next;
};

static int count = 1;
class CirList
{
	public:
		CirList(int n);
		int Delete(int p);
	private:
		Node *first;	
};

CirList::CirList(int n)
{
	Node *r, *s;
	Node *f = new Node;
	f->data = 1;
	r = f;
	for(int i = 2; i <= n; i++)
	{
		s = new Node;
		s->data = i;
		r->next = s; r = s;
	}
	r->next = f;
}
int CirList::Delete(int p)
{
	Node *r = first;
	while(count < p-1)
	{
		r = r->next;
		count++;
	}
	Node *q = r->next;
	int x = q->data;
	r->next = q->next;
	delete q;
	first = r->next;
	return x;
}
int main()
{
	int N,p;
	cin >> N >> p;
	CirList L(N);
	for(int i = 0; i < N; i++)
	{
		if(i == N-1) cout << L.Delete(p);
		else cout << L.Delete(p) << ' '; 
	}
	return 0;
}
